<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.flowerplatform.web.explorerChildrenProvider">
      <explorerChildrenProvider
            provider="org.flowerplatform.web.svn.explorer.SvnRepositories_OrganizationChildrenProvider">
         <nodeType
               nodeType="organization">
         </nodeType>
      </explorerChildrenProvider>
   </extension>
   <extension
         point="org.flowerplatform.web.explorerNodeDataProvider">
      <explorerNodeDataProvider
            provider="org.flowerplatform.web.svn.explorer.SvnRepositoriesNodeDataProvider">
         <nodeType
               nodeType="svnRepositories">
         </nodeType>
      </explorerNodeDataProvider>
   </extension>
   <extension
         point="org.flowerplatform.web.explorerChildrenProvider">
      <explorerChildrenProvider
            provider="org.flowerplatform.web.svn.explorer.SvnRepository_SvnRepositoriesChildrenProvider">
         <nodeType
               nodeType="svnRepositories">
         </nodeType>
      </explorerChildrenProvider>
   </extension>
   <extension
         point="org.flowerplatform.web.explorerNodeDataProvider">
      <explorerNodeDataProvider
            provider="org.flowerplatform.web.svn.explorer.SvnRepositoryNodeDataProvider">
         <nodeType
               nodeType="svnRepository">
         </nodeType>
      </explorerNodeDataProvider>
   </extension>
   <extension
         point="org.flowerplatform.web.explorerChildrenProvider">
      <explorerChildrenProvider
            provider="org.flowerplatform.web.svn.explorer.SvnFile_ChildrenProvider">
         <nodeType
               nodeType="svnRepository">
         </nodeType>
         <nodeType
               nodeType="svnFile">
         </nodeType>
      </explorerChildrenProvider>
   </extension>
   <extension
         point="org.flowerplatform.web.explorerNodeDataProvider">
      <explorerNodeDataProvider
            provider="org.flowerplatform.web.svn.explorer.SvnFileNodeDataProvider">
         <nodeType
               nodeType="svnFile">
         </nodeType>
      </explorerNodeDataProvider>
   </extension>
   <extension
         point="org.flowerplatform.communication.service">
      <service
            id="svnService"
            serviceClass="org.flowerplatform.web.svn.remote.SvnService">
      </service>
   </extension>
   <extension
         point="org.flowerplatform.web.explorerAdditionalNodePopulator">
      <explorerAdditionalNodePopulator
            populator="org.flowerplatform.web.svn.explorer.SvnFile_NodePopulator">
         <nodeType
               nodeType="svnFile">
         </nodeType>
      </explorerAdditionalNodePopulator>
   </extension>

</plugin>
