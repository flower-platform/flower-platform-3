<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
 <extension
         point="org.flowerplatform.codesync.code.modelAdapterFactorySet">
      <modelAdapterFactorySet
            id="org.flowerplatform.codesync.code.java.JavaModelAdapterFactorySet"
            technology="java"
            modelAdapterFactorySetClass="org.flowerplatform.codesync.code.java.JavaModelAdapterFactorySet">
      </modelAdapterFactorySet>
   </extension>

   <extension
         point="org.flowerplatform.editor.model.dragOnDiagramHandler">
      <dragOnDiagramHandler
            dragOnDiagramHandler="org.flowerplatform.editor.model.java.JavaDragOnDiagramHandler">
      </dragOnDiagramHandler>
   </extension>

	<extension
         point="org.flowerplatform.codesync.codeSyncAlgorithmRunner">
      <codeSyncAlgorithmRunner
            codeSyncAlgorithmRunnerClass="com.crispico.flower.mp.codesync.code.CodeSyncAlgorithmRunner"
            id="org.flowerplatform.codesync.code.CodeSyncAlgorithmRunner"
            technology="java">
      </codeSyncAlgorithmRunner>
   </extension>
 <extension
       point="org.flowerplatform.editor.model.contentAssist">
    <contentAssist
          contentAssist="org.flowerplatform.editor.model.java.JavaContentAssist">
    </contentAssist>
 </extension>
 <extension
       point="org.flowerplatform.editor.model.contentAssist">
    <contentAssist
          contentAssist="org.flowerplatform.editor.model.java.JavaCodeSyncElementContentAssist">
    </contentAssist>
 </extension>
 <extension
       point="org.flowerplatform.codesync.codeSyncElementDescriptor">
    <codeSyncElementDescriptor
          codeSyncType="javaClass"
          iconUrl="images/obj16/SyncClass.gif"
          label="Class">
       <childrenCodeSyncTypeCategory
             name="javaClassMember">
       </childrenCodeSyncTypeCategory>
    </codeSyncElementDescriptor>
 </extension>
 <extension
       point="org.flowerplatform.codesync.codeSyncElementDescriptor">
    <codeSyncElementDescriptor
          codeSyncType="javaOperation"
          iconUrl="images/obj16/SyncOperation_public.gif"
          label="Operation">
       <codeSyncTypeCategory
             name="javaClassMember">
       </codeSyncTypeCategory>
    </codeSyncElementDescriptor>
 </extension>
 <extension
       point="org.flowerplatform.codesync.codeSyncElementDescriptor">
    <codeSyncElementDescriptor
          codeSyncType="javaAttribute"
          iconUrl="images/obj16/SyncProperty_public.gif"
          label="Attribute">
       <codeSyncTypeCategory
             name="javaClassMember">
       </codeSyncTypeCategory>
    </codeSyncElementDescriptor>
 </extension>

</plugin>
