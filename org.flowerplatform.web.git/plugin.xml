<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.flowerplatform.web.explorerChildrenProvider">
      <explorerChildrenProvider
            provider="org.flowerplatform.web.git.explorer.GitRepositories_OrganizationChildrenProvider">
         <nodeType
               nodeType="organization">
         </nodeType>
      </explorerChildrenProvider>
   </extension>
   <extension
         point="org.flowerplatform.web.explorerNodeDataProvider">
      <explorerNodeDataProvider
            provider="org.flowerplatform.web.git.explorer.GitRepositoriesNodeDataProvider">
         <nodeType
               nodeType="gitRepositories">
         </nodeType>
      </explorerNodeDataProvider>
   </extension>
   <extension
         point="org.flowerplatform.web.explorerChildrenProvider">
      <explorerChildrenProvider
            provider="org.flowerplatform.web.git.explorer.Repository_GitRepositoriesChildrenProvider">
         <nodeType
               nodeType="gitRepositories">
         </nodeType>
      </explorerChildrenProvider>
   </extension>
   <extension
         point="org.flowerplatform.web.explorerNodeDataProvider">
      <explorerNodeDataProvider
            provider="org.flowerplatform.web.git.explorer.RepositoryNodeDataProvider">
         <nodeType
               nodeType="gitRepository">
         </nodeType>
      </explorerNodeDataProvider>
   </extension>
   <extension
         point="org.flowerplatform.web.explorerChildrenProvider">
      <explorerChildrenProvider
            provider="org.flowerplatform.web.git.explorer.VirtualItem_RepositoryChildrenProvider">
         <nodeType
               nodeType="gitRepository">
         </nodeType>
      </explorerChildrenProvider>
   </extension>
   <extension
         point="org.flowerplatform.web.explorerNodeDataProvider">
      <explorerNodeDataProvider
            provider="org.flowerplatform.web.git.explorer.VirtualItemDataProvider">
         <nodeType
               nodeType="gitLocalBranches">
         </nodeType>
         <nodeType
               nodeType="gitRemoteBranches">
         </nodeType>
         <nodeType
               nodeType="gitTags">
         </nodeType>
         <nodeType
               nodeType="gitRemotes">
         </nodeType> 
         <nodeType
               nodeType="gitWDirs">
         </nodeType>          
      </explorerNodeDataProvider>
   </extension>
   <extension
         point="org.flowerplatform.web.explorerChildrenProvider">
      <explorerChildrenProvider
            provider="org.flowerplatform.web.git.explorer.Ref_VirtualItemChildrenProvider">
         <nodeType
               nodeType="gitLocalBranches">
         </nodeType>
         <nodeType
               nodeType="gitRemoteBranches">
         </nodeType>
         <nodeType
               nodeType="gitTags">
         </nodeType>
      </explorerChildrenProvider>
   </extension>
   <extension
         point="org.flowerplatform.web.explorerNodeDataProvider">
      <explorerNodeDataProvider
            provider="org.flowerplatform.web.git.explorer.RefNodeDataProvider">
         <nodeType
               nodeType="gitLocalBranch">
         </nodeType>
         <nodeType
               nodeType="gitRemoteBranch">
         </nodeType>
         <nodeType
               nodeType="gitTag">
         </nodeType>
      </explorerNodeDataProvider>
   </extension>
   <extension
         point="org.flowerplatform.web.explorerChildrenProvider">
      <explorerChildrenProvider
            provider="org.flowerplatform.web.git.explorer.Remote_VirtualItemChildrenProvider">
         <nodeType
               nodeType="gitRemotes">
         </nodeType>
      </explorerChildrenProvider>
   </extension>
   <extension
         point="org.flowerplatform.web.explorerNodeDataProvider">
      <explorerNodeDataProvider
            provider="org.flowerplatform.web.git.explorer.RemoteNodeDataProvider">
         <nodeType
               nodeType="gitRemote">
         </nodeType>
      </explorerNodeDataProvider>
   </extension>
   <extension
         point="org.flowerplatform.web.explorerChildrenProvider">
      <explorerChildrenProvider
            provider="org.flowerplatform.web.git.explorer.WorkingDirectory_VirtualItemChildrenProvider">
         <nodeType
               nodeType="gitWDirs">
         </nodeType>
      </explorerChildrenProvider>
   </extension>
   <extension
         point="org.flowerplatform.web.explorerNodeDataProvider">
      <explorerNodeDataProvider
            provider="org.flowerplatform.web.git.explorer.GitFileNodeDataProvider">
         <nodeType
               nodeType="gitWDir">
         </nodeType>
         <nodeType
               nodeType="gitFile">
         </nodeType>
         <nodeType
               nodeType="gitFolder">
         </nodeType>
      </explorerNodeDataProvider>
   </extension>
   <extension
         point="org.flowerplatform.web.explorerChildrenProvider">
      <explorerChildrenProvider
            provider="org.flowerplatform.web.git.explorer.GitFile_WorkingDirectoryChildrenProvider">
         <nodeType
               nodeType="gitWDir">
         </nodeType>
         <nodeType
               nodeType="gitFile">
         </nodeType>
         <nodeType
               nodeType="gitFolder">
         </nodeType>
      </explorerChildrenProvider>
   </extension>
   <extension
         point="org.flowerplatform.communication.service">
      <service
            id="GitHistoryStatefulService"
            serviceClass="org.flowerplatform.web.git.history.remote.GitHistoryStatefulService">
      </service>
   </extension>
   <extension
         point="org.flowerplatform.communication.service">
      <service
            id="gitService"
            serviceClass="org.flowerplatform.web.git.GitService">
      </service>
   </extension>
   <extension
         point="org.flowerplatform.web.nodeTypeToCategoriesMapping">
      <nodeTypeToCategoriesMapping
            nodeType="gitRepositories">
         <nodeTypeCategory
               nodeTypeCategory="pathFragmentNamePointsToFile">
         </nodeTypeCategory>
      </nodeTypeToCategoriesMapping>
   </extension>
   <extension
         point="org.flowerplatform.web.nodeTypeToCategoriesMapping">
      <nodeTypeToCategoriesMapping
            nodeType="gitRepository">
         <nodeTypeCategory
               nodeTypeCategory="pathFragmentNamePointsToFile">
         </nodeTypeCategory>
      </nodeTypeToCategoriesMapping>
   </extension>
   <extension
         point="org.flowerplatform.web.nodeTypeToCategoriesMapping">
      <nodeTypeToCategoriesMapping
            nodeType="gitWDir">
         <nodeTypeCategory
               nodeTypeCategory="pathFragmentNamePointsToFile">
         </nodeTypeCategory>
         <nodeTypeCategory
               nodeTypeCategory="decoratableFile">
         </nodeTypeCategory>
      </nodeTypeToCategoriesMapping>
   </extension>
   <extension
         point="org.flowerplatform.web.nodeTypeToCategoriesMapping">
      <nodeTypeToCategoriesMapping
            nodeType="gitFile">
         <nodeTypeCategory
               nodeTypeCategory="pathFragmentNamePointsToFile">
         </nodeTypeCategory>
         <nodeTypeCategory
               nodeTypeCategory="decoratableFile">
         </nodeTypeCategory>
      </nodeTypeToCategoriesMapping>
   </extension>
   <extension
         point="org.flowerplatform.web.explorerAdditionalNodePopulator">
      <explorerAdditionalNodePopulator
            populator="org.flowerplatform.web.git.explorer.FSFile_GitNodePopulator">
         <nodeType
               nodeType="projFile">
         </nodeType>
         <nodeType
               nodeType="project">
         </nodeType>
      </explorerAdditionalNodePopulator>
   </extension>
</plugin>
