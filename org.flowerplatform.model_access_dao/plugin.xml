<plugin
      name="%pluginName"
      id="org.flowerplatform.model_access_dao"
      version="1.0.0"
      provider-name="%providerName">

   <requires>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="org.eclipse.emf.ecore" export="true"/>
   </requires>

   <runtime>
      <library name=".">
         <export name="*"/>
      </library>
   </runtime>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <package
            uri="model"
            class="org.flowerplatform.mode_access_dao.model.ModelPackage"
            genModel="emf_model/Model.genmodel"/>
   </extension>

</plugin>
